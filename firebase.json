{
  "hosting": {
    "public": "Public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "destination": "/home.html"
      },
      {
        "source": "/thanks",
        "destination": "/thanks.html"
      }
    ]
  }
}
